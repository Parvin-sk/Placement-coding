import java.util.Arrays;
class HelloWorld {
    public static void main(String[] args) {
      
        int n=5;
        int arr[]={1,3,5,4,2};
        Arrays.sort(arr);
        System.out.println(hello(n,arr));
            
    }
    public static int hello(int n,int[] arr){
        int count=0;
        for(int i=0;i<arr.length-1;i++){
        count+=arr[i+1]-arr[i];
        }
        return count;
        
    }
}
